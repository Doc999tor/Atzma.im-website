.test
  min-width 100% !important
  min-height: 300px
  background-color: white
.test:nth-child(2)
  background-color: red
.test:nth-child(3)
  background-color: purple
.content-box
  width 100% !important
  overflow hidden
.swiper-container
    width 100%
    margin-left auto
    margin-right auto
  .swiper-wrapper
    display flex
    width 100%
    .swiper-slide
      flex-shrink 0
      width 100%
      height 100%
